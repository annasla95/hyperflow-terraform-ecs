[
    {
      "essential": true,
      "image": "${image_url}",
      "memory": 256,
      "name": "${container_name}",
      "environment": [
        {
          "name": "AMQP_URL",
          "value": "amqp://${master_ip}:${rabbitmq_port}"
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "value": "${acess_key}"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": "${secret_key}"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "tmp-storage",
          "containerPath": "/tmp"
        }
      ]
    }
  ]

