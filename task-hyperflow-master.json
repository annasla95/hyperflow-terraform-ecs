[
    {
      "essential": true,
      "image": "${image_url}",
      "memory": 256,
      "name": "${container_name}",
      "portMappings": [
        {
          "containerPort": ${container_port},
          "hostPort": ${host_port}
        }
      ],
      "environment": [
        {
          "name": "INFLUX_DB",
          "value": "${influx_db_address}"
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "value": "${acess_key}"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": "${secret_key}"
        }
      ]
    }
  ]